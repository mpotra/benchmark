{
  "name": "benchmark",
  "version": "0.1.0",
  "description": "Benchmarking Tool for JavaScript",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib/*",
    "jslint": "eslint src",
    "jslint:test": "eslint test",
    "jslint:benchmark": "eslint benchmark",
    "build": "npm run clean && babel -d lib src/",
    "benchmark": "node babel-runner benchmark/run_benchmarks.js",
    "test": "node node_modules/.bin/babel-tape-runner test/run_tests.js | node_modules/.bin/tap-spec",
    "prepublish": "npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpotra/benchmark.git"
  },
  "keywords": [
    "benchmark",
    "high-resolution",
    "hrtime",
    "js",
    "javascript",
    "node",
    "node.js"
  ],
  "author": "Mihai Potra <mike@mpotra.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mpotra/benchmark/issues"
  },
  "homepage": "https://github.com/mpotra/benchmark#readme",
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.7",
    "babel-tape-runner": "^2.0.1",
    "debug": "^2.2.0",
    "eslint": "^2.7.0",
    "faucet": "0.0.1",
    "sinon": "^1.17.4",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "dependencies": {
    "bluebird": "^3.4.0"
  }
}
